{% set <FieldHandle> = entry.<FieldHandle> %}

{% if <FieldHandle> is not empty %}

  <h3><FieldName></h3>

  <ul class="<FieldHandle>">
    {% for option in <FieldHandle>.options %}
      <li {{ option.selected ? 'selected '}}data-value="{{ option.value }}">{{ option }}</li>
    {% endfor %}
  </ul>

{% endif %}
